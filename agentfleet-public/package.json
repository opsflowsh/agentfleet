{
  "name": "agentfleet",
  "version": "0.1.0",
  "description": "Build and orchestrate AI agent teams with a visual command center",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "cd src/web && vite",
    "dev:api": "cd src/api && nodemon server.js",
    "build": "npm run build:web && npm run build:api",
    "build:web": "cd src/web && vite build",
    "build:api": "cd src/api && tsc",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}'"
  },
  "keywords": [
    "ai-agents",
    "agent-orchestration",
    "openclaw",
    "ai-teams",
    "mission-control"
  ],
  "author": "AgentFleet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opsflowsh/agentfleet.git"
  },
  "bugs": {
    "url": "https://github.com/opsflowsh/agentfleet/issues"
  },
  "homepage": "https://agentfleet.sh",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}