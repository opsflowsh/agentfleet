# AgentFleet Configuration
# Copy this file to .env and update with your values

# Server Configuration
PORT=3000
NODE_ENV=development

# OpenClaw Configuration
OPENCLAW_GATEWAY_URL=http://localhost:39217
OPENCLAW_API_KEY=your-api-key-here

# Database (optional - defaults to local SQLite)
# DATABASE_URL=postgresql://user:password@localhost:5432/agentfleet

# Feature Flags
ENABLE_COMMUNITY_VOTING=true
ENABLE_ANALYTICS=true
MAX_AGENTS_PER_FLEET=10

# Security
SESSION_SECRET=your-session-secret-here
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Logging
LOG_LEVEL=info